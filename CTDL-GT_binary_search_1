//https://tek4.vn/khoa-hoc/cau-truc-du-lieu-va-thuat-toan/thuat-toan-tim-kiem-tuyen-tinh
#include <stdio.h>

int linear_search(int arr[], int n, int find) {
  for (int i = 0; i < n; i++)
    if (arr[i] == find)
      return i;
  return -1;
}

int main() {
  int arr[] = {4, 6, 8, 11, 2, 7};
  int x = 2;
  int n = sizeof(arr) / sizeof(arr[0]);
  int kq = linear_search(arr, n, x);
  (kq == -1) ? printf("Không tìm thấy") : printf("Giá trị cần tìm nằm tại vị trí: %d", kq);
}
